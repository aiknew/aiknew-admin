model LoginLog {
  id        String   @id @default(ulid())
  userName  String   @default("") @db.VarChar(20)
  userId    String   @default("") @db.VarChar(50)
  ip        String   @db.VarChar(45) // Supports IPv4 and IPv6
  location  String   @default("") @db.VarChar(100)
  userAgent String   @default("") @db.VarChar(500)
  os        String   @default("") @db.VarChar(50)
  browser   String   @default("") @db.VarChar(50)
  isSuccess Boolean  @default(false)
  createdAt DateTime @default(now())
}
