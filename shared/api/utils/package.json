{
  "name": "@aiknew/shared-api-utils",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@nestjs/common": "catalog:api",
    "@aiknew/shared-api-dtos": "workspace:*",
    "@aiknew/shared-api-enums": "workspace:*",
    "nestjs-i18n": "catalog:api"
  },
  "scripts": {
    "build": "pnpm tsc",
    "clean": "rm -rf ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "devDependencies": {
    "@aiknew/shared-ts-config": "workspace: *",
    "@aiknew/shared-api-types": "workspace: *",
    "@types/node": "catalog:common",
    "@types/jest": "catalog:api",
    "jest": "catalog:api",
    "ts-jest": "catalog:api"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
