{
  "name": "@aiknew/shared-ui-locales",
  "files": [
    "src/*.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./en": {
      "default": "./src/en.json"
    },
    "./zh-TW": {
      "default": "./src/zh-TW.json"
    },
    "./zh-CN": {
      "default": "./src/zh-CN.json"
    }
  },
  "peerDependencies": {
    "vue": "catalog:ui",
    "vue-i18n": "catalog:ui"
  }
}
