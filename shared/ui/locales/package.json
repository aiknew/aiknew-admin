{
  "name": "@aiknew/shared-ui-locales",
  "files": [
    "src/*.json"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./en": {
      "default": "./src/en.json"
    },
    "./zh-TW": {
      "default": "./src/zh-TW.json"
    },
    "./zh-CN": {
      "default": "./src/zh-CN.json"
    }
  },
  "peerDependencies": {
    "vue": "catalog:ui",
    "vue-i18n": "catalog:ui",
    "zod": "catalog:ui",
    "element-plus": "catalog:ui",
    "dayjs": "catalog:common",
    "altcha":"catalog:ui"
  },
  "devDependencies": {
    "@aiknew/shared-ts-config": "workspace:*"
  }
}
